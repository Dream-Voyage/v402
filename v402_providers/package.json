{
  "name": "@v402/providers",
  "version": "1.0.0",
  "description": "Advanced JavaScript/TypeScript provider SDK for v402 content monetization protocol with comprehensive enterprise-grade features",
  "keywords": [
    "v402",
    "x402",
    "payment",
    "micropayment",
    "blockchain",
    "web3",
    "content-monetization",
    "react",
    "vue",
    "angular",
    "svelte",
    "web-components",
    "typescript",
    "enterprise",
    "sdk",
    "fintech",
    "defi",
    "ugc",
    "creator-economy"
  ],
  "author": "v402 Team <team@v402.network>",
  "license": "MIT",
  "homepage": "https://github.com/v402/providers-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/v402/providers-js.git"
  },
  "bugs": {
    "url": "https://github.com/v402/providers-js/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "v402-provider": "dist/cli.js",
    "v402p": "dist/cli.js"
  },
  "files": [
    "dist",
    "src",
    "config",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/integrations/react.esm.js",
      "require": "./dist/integrations/react.js",
      "types": "./dist/integrations/react.d.ts"
    },
    "./vue": {
      "import": "./dist/integrations/vue.esm.js",
      "require": "./dist/integrations/vue.js",
      "types": "./dist/integrations/vue.d.ts"
    },
    "./angular": {
      "import": "./dist/integrations/angular.esm.js",
      "require": "./dist/integrations/angular.js",
      "types": "./dist/integrations/angular.d.ts"
    },
    "./svelte": {
      "import": "./dist/integrations/svelte.esm.js",
      "require": "./dist/integrations/svelte.js",
      "types": "./dist/integrations/svelte.d.ts"
    },
    "./vanilla": {
      "import": "./dist/integrations/vanilla.esm.js",
      "require": "./dist/integrations/vanilla.js",
      "types": "./dist/integrations/vanilla.d.ts"
    },
    "./web-components": {
      "import": "./dist/components/web-components.esm.js",
      "require": "./dist/components/web-components.js",
      "types": "./dist/components/web-components.d.ts"
    },
    "./workers": {
      "import": "./dist/workers/index.esm.js",
      "require": "./dist/workers/index.js",
      "types": "./dist/workers/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.esm.js",
      "require": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "dev:staging": "vite --mode staging",
    "build": "npm run build:clean && npm run build:types && npm run build:bundle && npm run build:workers",
    "build:clean": "rimraf dist coverage .nyc_output",
    "build:types": "tsc --project tsconfig.build.json",
    "build:bundle": "rollup -c rollup.config.js",
    "build:workers": "webpack --config webpack.workers.config.js",
    "build:watch": "rollup -c rollup.config.js --watch",
    "build:analyze": "npm run build && webpack-bundle-analyzer dist/stats.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "playwright test",
    "test:e2e": "cypress run",
    "test:performance": "lighthouse-ci autorun",
    "test:security": "npm audit && snyk test",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx,.vue --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx,.vue --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,vue,css,scss,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,vue,css,scss,md,json}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=PROJECT_TOKEN",
    "size": "size-limit",
    "size:analyze": "size-limit --why",
    "benchmark": "node scripts/benchmark.js",
    "benchmark:memory": "clinic doctor -- node scripts/benchmark.js",
    "benchmark:flame": "clinic flame -- node scripts/benchmark.js",
    "docs": "typedoc --out docs src/index.ts",
    "docs:serve": "serve docs -p 3001",
    "release": "standard-version && npm publish",
    "release:alpha": "standard-version --prerelease alpha && npm publish --tag alpha",
    "release:beta": "standard-version --prerelease beta && npm publish --tag beta",
    "precommit": "lint-staged && npm run type-check",
    "prepush": "npm run test && npm run build",
    "prepare": "husky install",
    "postinstall": "node scripts/postinstall.js",
    "cli": "node dist/cli.js",
    "dev:cli": "ts-node src/cli/index.ts",
    "validate": "npm run lint && npm run type-check && npm run test",
    "clean": "rimraf dist coverage .nyc_output node_modules/.cache",
    "reset": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "check-updates": "ncu -u",
    "security": "npm audit --audit-level moderate",
    "outdated": "npm outdated"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
    "@solana/web3.js": "^1.87.6",
    "@solana/wallet-adapter-base": "^0.9.23",
    "axios": "^1.6.2",
    "ethers": "^6.9.0",
    "web3": "^4.3.0",
    "web3-utils": "^4.0.7",
    "eventemitter3": "^5.0.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "decimal.js": "^10.4.3",
    "big.js": "^6.2.1",
    "lodash-es": "^4.17.21",
    "date-fns": "^2.30.0",
    "zod": "^3.22.4",
    "qs": "^6.11.2",
    "cross-fetch": "^4.0.0",
    "abort-controller": "^3.0.0",
    "p-retry": "^6.2.0",
    "p-queue": "^8.0.1",
    "p-debounce": "^4.0.0",
    "p-throttle": "^6.1.0",
    "lru-cache": "^10.1.0",
    "idb": "^7.1.1",
    "comlink": "^4.4.1",
    "workbox-core": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@angular/core": "^17.0.6",
    "@angular/common": "^17.0.6",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-docs": "^7.6.6",
