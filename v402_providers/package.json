    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vue": ">=3.0.0",
    "@angular/core": ">=15.0.0",
    "svelte": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "size-limit": [
    {
      "name": "Core SDK",
      "path": "dist/index.js",
      "limit": "100 KB"
    },
    {
      "name": "React Integration",
      "path": "dist/integrations/react.js",
      "limit": "50 KB"
    },
    {
      "name": "Vue Integration",
      "path": "dist/integrations/vue.js",
      "limit": "50 KB"
    },
    {
      "name": "Angular Integration",
      "path": "dist/integrations/angular.js",
      "limit": "60 KB"
    },
    {
      "name": "Svelte Integration",
      "path": "dist/integrations/svelte.js",
      "limit": "40 KB"
    },
    {
      "name": "Web Components",
      "path": "dist/components/web-components.js",
      "limit": "70 KB"
    }
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate"
    }
  },
  "volta": {
    "node": "18.19.0",
    "npm": "10.2.3"
  }
}
